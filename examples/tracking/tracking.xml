<?xml version="1.0" encoding="utf-8"?>
<tracking>
	<track id="trackTest1">
		<omniture>
			<track>
				<!-- this is always required. -->
				<pageName>test</pageName>
				<!--
				you can define any other properties.
				all defined properties are enumerated
				and defined on the "actionsource" component
				before firing.
				-->
				<prop6>test2</prop6>
				<eVar4>hello</eVar4>
			</track>
			<trackLink>
				<!-- track link tags will only ever have three parameters -->
				<url>http://www.whitehouse.ocom/</url> <!-- optional, if this isn't set it uses the "name" as the url --> 
				<type>o</type> <!-- o for custom, d for download, e for exit -->
				<name>adfasdf</name>
			</trackLink>
		</omniture>
	</track>
</tracking>